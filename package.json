{
	"name": "thor-web",
	"keywords": [
		"web",
		"framework"
	],
	"version": "1.4.30",
	"engines": {
		"node": ">=14.0.0"
	},
	"description": "An light weight web framework for node-js.",
	"bin": {
		"thor-web": "./bin/thor-web-init.mjs"
	},
	"main": "./dist/index.js",
	"repository": "https://gitee.com/thor.qin/thor-web.git",
	"author": "Thor Qin",
	"license": "MIT",
	"private": false,
	"peerDependencies": {
		"iconv-lite": "^0.6.3",
		"mime": "^3.0.0",
		"thor-time": "^1.1.1",
		"thor-tpl": "^1.1.8",
		"thor-validation": "^1.1.11",
		"uuid": "^8.3.2",
		"websocket": "^1.0.34"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.0.0",
		"@tsconfig/svelte": "^2.0.0",
		"@types/mime": "^2.0.3",
		"@types/node": "^17.0.18",
		"@types/uuid": "^8.3.4",
		"@types/websocket": "^1.0.5",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.5.1",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"shx": "^0.3.4",
		"svelte": "^3.0.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"tslib": "^2.0.0",
		"typescript": "^4.5.5",
		"svelte-material-icons": "^2.0.0"
	},
	"scripts": {
		"build-doc": "rollup -c",
		"watch-doc": "rollup -c -w",
		"build-cjs": "tsc -P tsconfig.json",
		"build": "rm -rf ./dist/* && rm -rf ./html/build/* && npm run build-cjs && npm run build-doc",
		"postbuild": "prettier --write ./dist "
	}
}
