
#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../thor-web/bin/thor-web-init.cjs" "$@"
  ret=$?
else
  node  "$basedir/../thor-web/bin/thor-web-init.cjs" "$@"
  ret=$?
fi

exit $ret
